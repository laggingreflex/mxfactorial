{
  "name": "mxfactorial-web",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.0.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "dayjs": "^1.7.5",
    "hoist-non-react-statics": "^2.5.5",
    "jsonschema": "^1.2.4",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "latest",
    "react-svg": "^4.1.8",
    "rxjs": "^6.3.3",
    "styled-components": "^3.3.3",
    "styled-is": "^1.1.3",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "NODE_PATH=./src react-scripts start",
    "build": "NODE_PATH=./src react-scripts build",
    "test": "react-scripts test --env=jsdom --modulePaths=./src",
    "loc:cov": "react-scripts test --forceExit --env=jsdom --modulePaths=./src --coverage",
    "ci:start": "start-server-and-test start http://localhost:3000 ci:test",
    "ci:test": "CI=true react-scripts test --forceExit --env=jsdom --modulePaths=./src --coverage",
    "test:e2e": "jest --detectOpenHandles --forceExit --config=e2e/config/index.js",
    "ci:e2e": "start-server-and-test start http://localhost:3000 test:e2e",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "ci:lint": "node node_modules/eslint/bin/eslint.js ./src"
  },
  "engines": {
    "node": "9.11.1",
    "npm": "5.8.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.12.1",
    "puppeteer": "^1.4.0",
    "start-server-and-test": "^1.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/App.js",
      "!src/setupTests.js",
      "!src/registerServiceWorker.js",
      "!src/config/**/*.js",
      "!src/mock/**/*.js",
      "!src/containers/**/*.js",
      "!src/lib/**/*.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
